            </div>
        </div>
        <!-- Footer -->
        <!-- Bootstrap core JavaScript-->
        <script src="~/Themes/AdminPanel/vendor/jquery/jquery.min.js"></script>
        <script src="~/Themes/AdminPanel/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/Themes/AdminPanel/vendor/jquery-easing/jquery.easing.min.js"></script>

        <!-- Custom scripts for all pages-->
